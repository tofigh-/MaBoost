\name{update.ada}
\alias{update.ada}

\title{
 Add more trees to an ada object
}

\description{
  \code{ada.update} updates the \code{ada} object to have additional trees given a new number of
   iterations.
}

\usage{
\method{update}{ada}(object, x, y, test.x, test.y = NULL, n.iter, \dots)
}

\arguments{
  \item{object}{object generated by the function \code{ada}. }
  \item{x}{x training data}
  \item{y}{training response}
  \item{test.x}{x testing data (optional)}
  \item{test.y}{the true labeling for this testing data (optional)}
  \item{n.iter}{ new number of iterations, must be provided and n.iter>iter}
  \item{\dots}{other arguments not used by this function.}
}

\value{
   updated \code{ada} object.
}

\seealso{
  \code{\link{ada.default}},\code{\link{summary.ada}},\code{\link{predict.ada}},
  \code{\link{plot.ada}},\code{\link{pairs.ada}}
}
\keyword{methods}